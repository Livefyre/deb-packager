<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
  <!--
    This jelly script is used for per-project configuration.

    See global.jelly for a general discussion about jelly script.
  -->

  <!--
    Creates a text field that shows the value of the "name" property.
    When submitted, it will be passed to the corresponding constructor parameter.
  -->
  <f:entry title="Package Name" field="packageName">
    <f:textbox />
  </f:entry>

  <f:entry title="Version Format" field="versionFormat">
    <f:textbox name="versionFormat" />
  </f:entry>

  <f:entry title="Move workspace to" field="moveWorkspacePath">
    <f:textbox />
  </f:entry>

  <f:entry title="Dependencies" field="dependencies">
    <f:textbox />
  </f:entry>

  <f:entry title="Maintainer" field="maintainer">
    <f:textbox />
  </f:entry>

  <f:entry title="preinst script:" field="preinst">
    <f:textarea />
  </f:entry>

  <f:entry title="postinst script:" field="postinst">
    <f:textarea />
  </f:entry>

  <f:entry title="prerm script:" field="prerm">
    <f:textarea />
  </f:entry>

  <f:entry title="postrm script:" field="postrm">
    <f:textarea />
  </f:entry>
</j:jelly>
